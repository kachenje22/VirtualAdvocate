@{
    ViewBag.Title = "Thank You Page";
}

<h2>THANK YOU</h2>
    <hr />
<div class="container">
    <div class="row">
        <div class="col-lg-6">
            <div class="alert alert-dismissible alert-success">
                <button type="button" class="close" data-dismiss="alert">×</button>
                <h4>Thank you for Registering</h4>
                <p>Account is waiting approval from Administrator</p>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <h4 class="text-success">Thank you for Registering</h4>
                <p>Account Approval mail will be sent to your registered mail id from SUPER ADMIN</p>
            </div>
        </div>
    </div>
</div>
    


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
